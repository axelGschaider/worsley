.spot {
  list-style-type: none;
  background-color: #f5f5f5;
  border: 1px solid;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  padding: 5px;
  margin-bottom: 10px;
}

.points {
  font-size: 18px;
}

.points .counter {
  color: red;
}

.newButton {
  font-size: 20px;
  background-color: rgb(144, 196, 144);
  border: 1px solid;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  padding: 5px;
  margin-top: 5px;
}
